<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TekVan.SignUp">
    <StackLayout Margin="10">
        <Entry x:Name="txtUsername" Placeholder="Username"/>
        <Entry x:Name="txtPassword" IsPassword="True" Placeholder="Password"/>
        <Entry x:Name="txtEmail" Placeholder="Email"/>
        <Entry x:Name="txtAddress" Placeholder="Address"/>
        <StackLayout Orientation="Horizontal" HorizontalOptions="Center">
            <Button Text="Sign Up" Clicked="SignUp_OnClicked"></Button>
            <Button Text="Back" Clicked="BackButton_OnClicked"></Button>
        </StackLayout>
    </StackLayout>
</ContentPage>